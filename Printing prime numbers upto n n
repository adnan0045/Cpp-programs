#include <iostream>
using namespace std;
bool isPrime (int x){
    bool f = true;
    for (int k=2;k<=x-1;k++){
        if (x%k==0)
        f = false;
    }
    return f;
}
void result (int n){
    for (int i=2;i<=n;i++){
        if (isPrime(i))
        cout<<i<<endl;
    }
}
int main (){
    int n;
    cin>>n;
    result(n);
    return 0;
}
